{
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "Forms\\": "Forms/",
            "Controllers\\": "Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
  "require": {
    "ext-pdo": "*"
  }
}
